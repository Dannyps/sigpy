{
    "url": "study_plan",
    "attributes": {
        "years": {
            "model": "year",
            "list": "True",
            "css": "a#bloco_acurr_ShowOrHide + div.caixa",
            "attributes": {
                "name": {
                    "regex": "class=\"caixa\" id=\"\\d*ano_(\\d+)\""
                },
                "semesters": {
                    "model": "semester",
                    "list": "True",
                    "xpath": ".",
                    "attributes": {
                        "name": {
                            "regex": "colspan=\"7\">(\\d+).*"
                        },
                        "subjects": {
                            "model": "subject",
                            "list": "True",
                            "css": "tr.i,tr.p",
                            "attributes": {
                                "code": {
                                    "xpath": ".//td[1]"
                                },
                                "initials": {
                                    "xpath": ".//td[2]"
                                },
                                "name": {
                                    "xpath": ".//td[3]"
                                },
                                "observations": {
                                    "xpath": ".//td[4]"
                                },
                                "ects": {
                                    "xpath": ".//td[5]"
                                },
                                "id": {
                                    "regex": "ucurr_geral.ficha_uc_view\\?pv_ocorrencia_id=(\\d+)"
                                }
                            }
                        }
                    }
                }
            }
        },
        "optionals": {
            "model": "subject",
            "list": "True",
            "css": ".caixa[id^=\"div_id_\"] tr.i,.caixa[id^=\"div_id_\"] tr.p",
            "attributes": {
                "code": {
                    "xpath": ".//td[1]"
                },
                "initials": {
                    "xpath": ".//td[2]"
                },
                "name": {
                    "xpath": ".//td[3]"
                },
                "ects": {
                    "xpath": ".//td[4]"
                },
                "year": {
                    "xpath": ".//td[5]"
                },
                "semester": {
                    "xpath": ".//td[6]"
                },
                "id": {
                    "regex": "ucurr_geral.ficha_uc_view\\?pv_ocorrencia_id=(\\d+)"
                }
            }
        }
    }
}